<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.myapplication.todo">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.Todo">

        <activity android:name="crudmoodstatus.MainActivity" />
        <activity android:name="crudmoodstatus.ShowGraphActivity" />
        <activity android:name="crudmoodstatus.ViewCalendarActivity" />
        <activity android:name="crudmoodstatus.ViewDayOfCalendarActivity" />
        <activity android:name="crudmoodstatus.ViewEntryActivity" />
        <activity android:name="crudmoodstatus.ViewEntryListActivity" />

        <activity android:name="activity.SuggestActivity" />
        <activity android:name="activity.MainActivity" />
        <activity android:name="activity.RateActivity" />
        <activity android:name="activity.SetReminderActivity" />
        <activity android:name="activity.HomeSetReminderActivity" />
        <activity android:name="activity.RMainActivity" />
        <activity android:name="activity.RRateActivity" />
        <activity android:name="activity.RResourcesActivity" />

        <receiver android:name="activity.BcastRcver"/>

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResourcesActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".QuizQuestionActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".OnbordingActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>